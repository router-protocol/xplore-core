{
  "name": "router-aggregator-ts-core",
  "version": "1.0.0",
  "description": "TypeScript SDK for router aggregation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "oxlint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "bun run build"
  },
  "keywords": ["sdk", "typescript", "router", "aggregator"],
  "author": "",
  "license": "ISC",
  "packageManager": "bun@1.1.0",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "oxlint": "^0.13.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
